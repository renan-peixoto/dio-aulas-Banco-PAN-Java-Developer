    </dependency>
    <dependency>
      <groupId>${project.groupId}</groupId>
      <artifactId>quick-strat-maven-biz-api</artifactId>
    </dependency>
    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-maven-plugin</artifactId>
        <configuration>
          <mainClass>org.example.ctl.CtlQuickstartApplication</mainClass>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
